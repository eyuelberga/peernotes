{
  "name": "peer-notes-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "yarn env:gen && react-scripts start",
    "start:test": "yarn env:gen && react-scripts -r @cypress/instrument-cra start",
    "build": "sh -ac '. ./.env; react-scripts build'",
    "build:netlify": "CI=false  SKIP_PREFLIGHT_CHECK=true && react-scripts build",
    "build:test": "sh -ac '. ./.env; CYPRESS_INSTRUMENT_PRODUCTION=true react-scripts -r @cypress/instrument-cra build'",
    "env:gen": "chmod +x ./env.sh && ./env.sh && cp env-config.js ./public/",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint:fix": "eslint src/**/*.ts{,x} --fix",
    "lint": "eslint src/**/*.ts{,x}",
    "storybook": "start-storybook -p 6006 -s public",
    "storybook:build": "build-storybook -s public"
  },
  "dependencies": {
    "@apollo/client": "^3.3.6",
    "@auth0/auth0-react": "^1.2.0",
    "@chakra-ui/react": "^1.4.2",
